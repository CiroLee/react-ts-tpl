{
  "name": "react-ts-tpl",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --config .eslintrc.js",
    "lint:css": "stylelint src/**/*.{less,css,sass,scss}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{jsx,tsx,ts,js,json,md}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{less,css,sass,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.6.1",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "commitlint": "^16.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "stylelint": "^14.8.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
